{
  "version": 2,
  "builds": [
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "link-preview/pages/[slug].js",
      "use": "@vercel/next"
    },
    {
      "src": "link-preview/api/preview.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/berita-home.html",
      "dest": "/api/server.js"
    },
    {
      "src": "/artikel-home.html",
      "dest": "/api/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/api/preview",
      "dest": "/link-preview/api/preview.js"
    },
    {
      "src": "/(.*)",
      "dest": "/link-preview/pages/[slug].js"
    }
  ]
}